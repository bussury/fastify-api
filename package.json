{
  "name": "fastify-api",
  "type": "module",
  "private": true,
  "scripts": {
    "migrate": "knex migrate:latest --knexfile ./database/knexfile.js ",
    "seed": "knex seed:run --knexfile database/knexfile.js",
    "dev": "node server.mjs",
    "watch": "nodemon server.mjs",
    "build": "node server.mjs build",
    "serve": "cross-env NODE_ENV=production node server.mjs",
    "debug": "node --inspect-brk server",
    "test": "tap resources/test/test.js"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "connect-session-knex": "^2.1.1",
    "faker": "^5.5.3",
    "fastify-api": "^0.2.0",
    "fastify-cookie": "^5.6.0",
    "fastify-formbody": "^5.2.0",
    "fastify-multipart": "^5.3.1",
    "fastify-sensible": "^3.1.2",
    "fastify-session": "^5.2.1",
    "fastify-vite": "^2.3.1",
    "fastify-vite-vue": "^3.0.0",
    "http-errors": "^2.0.0",
    "knex": "^1.0.5",
    "mysql2": "^2.3.3",
    "objection": "^3.0.1",
    "vite": "^2.9.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "fastify": "^3.28.0",
    "fastify-autoload": "^3.11.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  }
}
