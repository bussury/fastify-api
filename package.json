{
  "name": "fastify-api",
  "type": "module",
  "private": true,
  "author": "Bussury A.Omary",
  "scripts": {
    "migrate": "knex migrate:latest --knexfile ./database/knexfile.dev.js ",
    "seed": "knex seed:run --knexfile database/knexfile.dev.js",
    "dev": "node main.mjs",
