{
  "name": "fastify-api",
  "type": "module",
  "private": true,
  "scripts": {
    "migrate": "knex migrate:latest --knexfile ./database/knexfile.js ",
    "seed": "knex seed:run --knexfile database/knexfile.js",
    "dev": "node server.mjs",
    "watch": "nodemon server.mjs",
    "build": "node server.mjs build",
    "serve": "cross-env NODE_ENV=production node server.mjs",
    "debug": "node --inspect-brk server",
    "test": "tap resources/test/test.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "connect-session-knex": "^2.1.1",
    "faker": "^6.6.6",
    "fastify-cookie": "^5.7.0",
    "fastify-formbody": "^5.3.0",
    "fastify-multipart": "^5.4.0",
    "fastify-sensible": "^3.2.0",
    "fastify-session": "^5.2.1",
    "http-errors": "^2.0.0",
    "knex": "^2.0.0",
    "mysql2": "^2.3.3",
    "objection": "^3.0.1",
    "vite": "^2.9.8",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "fastify": "^3.29.0",
    "fastify-autoload": "^3.13.0",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  }
}
