    "@fastify/sensible": "^5.5.0",
    "@fastify/session": "^10.5.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "connect-session-knex": "^3.0.1",
    "http-errors": "^2.0.0",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "mysql2": "^3.6.2",
    "objection": "^3.1.2",
    "pino": "^8.16.1"
  },
  "devDependencies": {
    "@fastify/autoload": "^5.8.0",
    "cross-env": "^7.0.3",
